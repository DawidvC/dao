
io.writeln( "here\n" )

if( DaoMake::IsUnix() ){
	cflags = DaoMake::Shell( "llvm-config --cppflags" )
	lflags = DaoMake::Shell( "llvm-config --ldflags" )
	libs = DaoMake::Shell( "llvm-config --libs" )
	if( libs == "" ) return;

	llvm = DaoMake::Project( "LLVM" )
	llvm.ExportCompilingFlags( cflags.chop() )
	llvm.ExportLinkingFlags( libs.chop() )
	llvm.ExportLinkingFlags( lflags.chop() )

	io.writeln( llvm )
}
