load CharType;




var mbs_alnum = ""
var mbs_alnum_neg = ""

for( ch = 1 : 127 ){
	if( isalnum( ch ) ){
		mbs_alnum += ch
	}else{
		mbs_alnum_neg += ch
	}
}

@[test()]
m1 = mbs_alnum.match( "[%a%d]+" )
m2 = mbs_alnum_neg.match( "[^%a%d]+" )
res = ( m1.start, m1.end, m2.start, m2.end )
@[test()]
@[test()]
( 0, mbs_alnum.size()-1, 0, mbs_alnum_neg.size()-1 )
@[test()]



