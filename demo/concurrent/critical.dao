

count = 0;
mt.iterate( 10, 4 )::{
	io.writeln( 'outside of critical section', Y );
	mt.critical::{[]
		io.writeln( 'in critical section', Y );
		for( i = 1 : 100000 ) count += 1;
	}
}
io.writeln( count );

count = 0;
mutex = mt.mutex();

mt.iterate( 10, 4 )::{
	mutex.protect::{
		io.writeln( 'in protected section' );
		for( i = 1 : 100000 ) count += 1;
	}
}
io.writeln( count );
