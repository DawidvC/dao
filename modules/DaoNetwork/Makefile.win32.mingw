
CC = gcc
INCLUDE = -I. -I..\..\kernel
LIB = -lwsock32 -L..\.. -ldao
CFLAG = -c -Wall -O2
LFLAG = -s -shared 

TARGET = DaoNetwork.dll

all: $(TARGET)

daoNetwork.o: daoNetwork.c
	$(CC) $(CFLAG) $(INCLUDE) daoNetwork.c
	
$(TARGET): daoNetwork.o
	$(CC) $(LFLAG) daoNetwork.o $(LIB) -o $(TARGET)

bind: clean

clean:
	del /Q *.o
