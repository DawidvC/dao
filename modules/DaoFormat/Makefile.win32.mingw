
CC = gcc
INCLUDE = -I. -I..\..\kernel
LIB = -L..\.. -ldao
CFLAG = -c -Wall -O2
LFLAG = -s -shared

TARGET = DaoFormat.dll

all: $(TARGET)

daoFormat.o: daoFormat.c
	$(CC) $(CFLAG) $(INCLUDE) daoFormat.c
	
$(TARGET): daoFormat.o
	$(CC) $(LFLAG) daoFormat.o $(LIB) -o $(TARGET)

bind: clean

clean:
	del /Q *.o
