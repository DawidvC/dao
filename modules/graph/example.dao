
load graph;

graph = Graph( $int );
node1 = graph.add_node();
node2 = graph.add_node();
node3 = graph.add_node();
edge1 = graph.add_edge( node1, node2 );
edge2 = graph.add_edge( node2, node3 );
edge3 = graph.add_edge( node1, node3 );
edge1.set_weight( 2.5 );
edge2.set_weight( 3 );
edge3.set_weight( 1 );

io.writeln( edge1.nodes() );
io.writeln( graph.nodes(), graph.edges() )

io.writeln( graph.maxflow( node1, node3 ) );
