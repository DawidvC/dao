
CC = gcc
INCLUDE = -I. -I..\..\kernel
LIB = -L..\.. -ldao
CFLAG = -c -Wall -O2
LFLAG = -s -shared 

TARGET = DaoInode.dll

all: $(TARGET)

daoInode.o: daoInode.c
	$(CC) $(CFLAG) $(INCLUDE) daoInode.c
	
$(TARGET): daoInode.o
	$(CC) $(LFLAG) daoInode.o $(LIB) -o $(TARGET)

bind: clean

clean:
	del /Q *.o
